{
  "$schema": "./node_modules/@nrwl/schematics/src/schema.json",
  "project": {
    "name": "angularlicious-workspace",
    "npmScope": "angularlicious",
    "latestMigration": "20180424-add-tsconfig-tools"
  },
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "./tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "apps": [
    {
      "name": "actions",
      "root": "libs/actions/src",
      "test": "../../../test.js",
      "appRoot": "",
      "tags": []
    },
    {
      "name": "core",
      "root": "libs/core/src",
      "test": "../../../test.js",
      "appRoot": "",
      "tags": []
    },
    {
      "name": "foundation",
      "root": "libs/foundation/src",
      "test": "../../../test.js",
      "appRoot": "",
      "tags": []
    },
    {
      "name": "logging",
      "root": "libs/logging/src",
      "test": "../../../test.js",
      "appRoot": "",
      "tags": []
    },
    {
      "name": "markdown-editor",
      "root": "libs/markdown-editor/src",
      "test": "../../../test.js",
      "appRoot": "",
      "tags": []
    },
    {
      "name": "rules-engine",
      "root": "libs/rules-engine/src",
      "test": "../../../test.js",
      "appRoot": "",
      "tags": []
    },
    {
      "name": "security",
      "root": "libs/security/src",
      "test": "../../../test.js",
      "appRoot": "",
      "tags": []
    },
    {
      "name": "$workspaceRoot",
      "root": ".",
      "appRoot": ""
    }
  ],
  "defaults": {
    "schematics": {
      "collection": "@nrwl/schematics"
    },
    "styleExt": "css",
    "component": {}
  },
  "warnings": {
    "typescriptMismatch": false
  }
}
