{
  "compileOnSave": false,
  "compilerOptions": {
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "baseUrl": ".",
    "paths": {
      "@angularlicious/rules-engine": [
        "libs/rules-engine/src/index.ts"
      ],
      "@angularlicious/foundation": [
        "libs/foundation/src/index.ts"
      ],
      "@angularlicious/core": [
        "libs/core/src/index.ts"
      ],
      "@angularlicious/actions": [
        "libs/actions/src/index.ts"
      ],
      "@angularlicious/logging": [
        "libs/logging/src/index.ts"
      ],
      "@angularlicious/firebase": [
        "libs/firebase/src/index.ts"
      ],
      "@angularlicious/configuration": [
        "libs/configuration/src/index.ts"
      ],
      "@angularlicious/error-handling": [
        "libs/error-handling/src/index.ts"
      ],
      "http-service/*": [
        "dist/http-service/*"
      ],
      "@angularlicious/http-service": [
        "libs/http-service/src/index.ts"
      ],
      "@angularlicious/security": [
        "libs/security/src/index.ts"
      ],
      "@angularlicious/markdown-editor": [
        "libs/markdown-editor/src/index.ts"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "tmp"
  ]
}
