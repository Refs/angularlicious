{
  "name": "@angularlicious/core",
  "version": "6.0.0",
  "description": "An Angular custom library that contains core application items for Angularlicious Angular applications.",
  "ngPackage": {
    "lib": {
      "entryFile": "./index.ts",
      "umdModuleIds": {
        "@angularlicious/rules-engine": "angularliciousRulesEngine",
        "@angularlicious/logging": "angularliciousLogging",
        "@angularlicious/foundation": "angularliciousFoundation"
      }
    },
    "dest": "./../../dist/@angularlicious/core"
  },
  "keywords": [
    "Angular",
    "Build Motion",
    "Matt Vaughn",
    "angularlicious",
    "@angularlicious",
    "#Angularlicious"
  ],
  "author": "Matt Vaughn",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/angularlicious/angularlicious.git"
  },
  "peerDependencies": {
    "@angular/http": "^6.0.7",
    "@angular/router": "^6.0.7",
    "@types/google.analytics": "0.0.36",
    "@angularlicious/actions": "^6.0.0",
    "@angularlicious/rule-engine": "^6.0.0"
  }
}
