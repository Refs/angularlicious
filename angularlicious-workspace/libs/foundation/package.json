{
    "name": "@angularlicious/foundation",
    "version": "5.0.1",
    "description": "An Angular custom module that contains foundation elements for angularlicious Angular applications. Basically, the framework for angularlicious NG.",
    "ngPackage": {
        "lib": {
            "entryFile": "./index.ts",
            "umdModuleIds": {
                "angular-rules-engine": "angularRulesEngine",
                "angular-actions": "angularActions",
                "@angularlicious/logging": "angularliciousLogging"
            }
        },
        "dest": "./../../dist/@angularlicious/foundation"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/angularlicious/angularlicious.git"
    },
    "keywords": [
        "angularlicious",
        "Angular",
        "Angularlicious",
        "Angularlicio.us",
        "Matt Vaughn"
    ],
    "author": "matt.vaughn@angularlicious.com www.angularlicious.com",
    "license": "MIT",
    "peerDependencies": {
        "@angular/common": "^5.1.3",
        "@angular/core": "^5.1.3",
        "@angular/http": "^5.1.3",
        "@angularlicious/logging": "^3.1.2",
        "angular-actions": "^2.0.0",
        "angular-rules-engine": "^2.2.0",
        "rxjs": "^5.5.0"
    }
}
