{
  "name": "angularlicious-workspace",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --project=component-event",
    "version": "node version.js",
    "build": "ng build",
    "build:firebase-auth": "ng serve --project=firebase-auth --watch",
    "build:web": "ng build --project=angularlicious-web --prod && npm run publish:web",
    "build:debug": "ng build --project=angularlicious-web",
    "publish:web": "npm run clean:web && sync-glob -d false 'dist/apps/angularlicious-web/**' ../../../../angularlicio.us/source/BuildMotion.Angularlicious.solution/BuildMotion.Angularlicious.WebHost",
    "clean:web": "npm run clean1:web && npm run clean2:web && npm run clean3:web && npm run clean4:web && npm run clean5:web && npm run clean6:web",
    "clean1:web": "del-cli -f ./../../angularlicio.us/source/BuildMotion.Angularlicious.solution/BuildMotion.Angularlicious.WebHost/*.js.map",
    "clean2:web": "del-cli -f ./../../angularlicio.us/source/BuildMotion.Angularlicious.solution/BuildMotion.Angularlicious.WebHost/*.bundle.js",
    "clean3:web": "del-cli -f ./../../angularlicio.us/source/BuildMotion.Angularlicious.solution/BuildMotion.Angularlicious.WebHost/*.css",
    "clean4:web": "del-cli -f ./../../angularlicio.us/source/BuildMotion.Angularlicious.solution/BuildMotion.Angularlicious.WebHost/*.js",
    "clean5:web": "del-cli -f ./../../angularlicio.us/source/BuildMotion.Angularlicious.solution/BuildMotion.Angularlicious.WebHost/index.html",
    "clean6:web": "del-cli -f ./../../angularlicio.us/source/BuildMotion.Angularlicious.solution/BuildMotion.Angularlicious.WebHost/assets/**",
    "package:libs": "npm run package:rules-engine && npm run package:configuration && npm run package:logging && npm run package:actions && npm run package:foundation && npm run package:core && npm run package:security && npm run package:markdown-editor && npm run package:contentful",
    "package:configuration": "ng-packagr -p libs/configuration/ng-package.json && npm run copy:configuration",
    "package:logging": "ng-packagr -p libs/logging/ng-package.json && npm run copy:logging",
    "package:rules-engine": "ng-packagr -p libs/rules-engine/ng-package.json && npm run copy:rules-engine",
    "package:actions": "ng-packagr -p libs/actions/ng-package.json && npm run copy:actions",
    "package:foundation": "ng-packagr -p libs/foundation/ng-package.json && npm run copy:foundation",
    "package:foundation99": "ng build --project=foundation --prod && npm run copy:foundation",
    "package:core": "ng-packagr -p libs/core/ng-package.json && npm run copy:core",
    "package:security": "ng-packagr -p libs/security/ng-package.json && npm run copy:security",
    "package:contentful": "ng-packagr -p libs/contentful/ng-package.json && npm run copy:contentful",
    "package:markdown-editor": "ng-packagr -p libs/markdown-editor/ng-package.json && npm run copy:markdown-editor",
    "copy:configuration": "sync-glob 'dist/@angularlicious/configuration/' node_modules/@angularlicious/configuration",
    "copy:logging": "sync-glob 'dist/@angularlicious/logging/' node_modules/@angularlicious/logging",
    "copy:rules-engine": "sync-glob 'dist/@angularlicious/rules-engine/' node_modules/@angularlicious/rules-engine",
    "copy:actions": "sync-glob 'dist/@angularlicious/actions/' node_modules/@angularlicious/actions",
    "copy:foundation": "sync-glob 'dist/@angularlicious/foundation/' node_modules/@angularlicious/foundation",
    "copy:core": "sync-glob 'dist/@angularlicious/core/' node_modules/@angularlicious/core",
    "copy:security": "sync-glob 'dist/@angularlicious/security/' node_modules/@angularlicious/security",
    "copy:contentful": "sync-glob 'dist/@angularlicious/contentful/' node_modules/@angularlicious/contentful",
    "copy:markdown-editor": "sync-glob 'dist/@angularlicious/markdown-editor/' node_modules/@angularlicious/markdown-editor",
    "test": "ng test",
    "lint": "./node_modules/.bin/nx lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "./node_modules/.bin/nx affected:apps",
    "affected:build": "./node_modules/.bin/nx affected:build",
    "affected:e2e": "./node_modules/.bin/nx affected:e2e",
    "affected:test": "./node_modules/.bin/nx affected:test",
    "affected:lint": "./node_modules/.bin/nx affected:lint",
    "affected:dep-graph": "./node_modules/.bin/nx affected:dep-graph",
    "format": "./node_modules/.bin/nx format:write",
    "format:write": "./node_modules/.bin/nx format:write",
    "format:check": "./node_modules/.bin/nx format:check",
    "update": "ng update @nrwl/schematics",
    "update:check": "ng update",
    "workspace-schematic": "./node_modules/.bin/nx workspace-schematic",
    "dep-graph": "./node_modules/.bin/nx dep-graph",
    "help": "./node_modules/.bin/nx help"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "7.1.1",
    "@angular/core": "7.1.1",
    "@angular/compiler": "^7.1.1",
    "@angular/fire": "^5.1.1",
    "@angular/forms": "7.1.1",
    "@angular/platform-browser": "7.1.1",
    "@angular/platform-browser-dynamic": "7.1.1",
    "@angular/router": "7.1.1",
    "core-js": "^2.5.4",
    "firebase": "^5.6.0",
    "ngx-loggly-logger": "^6.0.0",
    "rxjs": "6.3.3",
    "rxjs-compat": "6.3.3",
    "simplemde": "^1.11.2",
    "typescript-dotnet-commonjs": "^4.10.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/schematics": "^7.1.1",
    "@angular/cli": "^7.1.1",
    "@angular/compiler-cli": "^7.1.1",
    "@angular/language-service": "7.1.1",
    "@nrwl/nx": "^7.1.1",
    "@nrwl/schematics": "^7.1.1",
    "@types/jasmine": "~3.3.1",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "~10.12.12",
    "codelyzer": "~4.5.0",
    "del-cli": "^1.1.0",
    "git-describe": "^4.0.3",
    "jasmine-core": "~3.3.0",
    "jasmine-marbles": "0.4.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.3",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.4",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^4.4.5",
    "prettier": "1.15.3",
    "protractor": "~5.4.1",
    "sync-glob": "^1.3.8",
    "ts-node": "~7.0.1",
    "tsickle": "0.34.0",
    "tslib": "^1.7.1",
    "tslint": "~5.11.0",
    "typescript": "^3.2.1"
  }
}
